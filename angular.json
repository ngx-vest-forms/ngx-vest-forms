{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-vest-forms": {
      "projectType": "library",
      "root": "projects/ngx-vest-forms",
      "sourceRoot": "projects/ngx-vest-forms/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-vest-forms/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-vest-forms/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-vest-forms/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "storybook": {
          "builder": "@storybook/angular:start-storybook",
          "options": {
            "styles": ["projects/ngx-vest-forms/.storybook/styles.scss"],
            "configDir": "projects/ngx-vest-forms/.storybook",
            "browserTarget": "ngx-vest-forms:build",
            "compodoc": true,
            "compodocArgs": ["-e", "json", "-d", "projects/ngx-vest-forms"],
            "port": 6006
          }
        },
        "build-storybook": {
          "builder": "@storybook/angular:build-storybook",
          "options": {
            "configDir": "projects/ngx-vest-forms/.storybook",
            "styles": ["projects/ngx-vest-forms/.storybook/styles.scss"],
            "browserTarget": "ngx-vest-forms:build",
            "compodoc": true,
            "compodocArgs": ["-e", "json", "-d", "projects/ngx-vest-forms"],
            "outputDir": "dist/storybook/ngx-vest-forms"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/ngx-vest-forms/**/*.ts",
              "projects/ngx-vest-forms/**/*.html"
            ],
            "eslintConfig": "projects/ngx-vest-forms/eslint.config.mjs"
          }
        }
      }
    },
    "examples": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/examples",
      "sourceRoot": "projects/examples/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": {
              "base": "dist/examples"
            },
            "index": "projects/examples/src/index.html",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/examples/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/examples/src/favicon.ico",
              "projects/examples/src/assets"
            ],
            "styles": ["projects/examples/src/styles.scss"],
            "scripts": [],
            "browser": "projects/examples/src/main.ts"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "examples:build:production"
            },
            "development": {
              "buildTarget": "examples:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "examples:build"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/examples/**/*.ts",
              "projects/examples/**/*.html"
            ],
            "eslintConfig": "projects/examples/eslint.config.mjs"
          }
        },
        "test": {
          "builder": "@analogjs/vitest-angular:test"
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
