{
  "branches": [
    "master",
    {
      "name": "release/v1.x",
      "range": "1.x.x",
      "channel": "release-1.x"
    },
    {
      "name": "release/v2.x",
      "range": "2.x.x",
      "channel": "release-2.x"
    },
    {
      "name": "beta",
      "prerelease": true
    },
    {
      "name": "rc",
      "prerelease": "rc"
    },
    {
      "name": "alpha",
      "prerelease": true
    },
    {
      "name": "feat/*",
      "prerelease": "feat-${name.replace(/^feat\//, '').replace(/[^a-zA-Z0-9-_]/g, '')}"
    },
    {
      "name": "fix/*",
      "prerelease": "fix-${name.replace(/^fix\//, '').replace(/[^a-zA-Z0-9-_]/g, '')}"
    }
  ],
  "plugins": [
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    [
      "@semantic-release/npm",
      {
        "pkgRoot": "dist/ngx-vest-forms"
      }
    ],
    "@semantic-release/git",
    "@semantic-release/github"
  ]
}
